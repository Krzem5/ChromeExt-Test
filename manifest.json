{
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "test ext title"
	},
	"content_scripts": [
		{
			"js": [
				"content.js"
			],
			"matches": [
				"<all_urls>"
			]
		}
	],
	"icons": {
		"48": "icon.png"
	},
	"manifest_version": 2,
	"name": "test ext",
	"permissions": [
		"tabs",
		"<all_urls>"
	],
	"version": "1.0"
}